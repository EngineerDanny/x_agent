root       ::= object
object     ::= "{" ws "\"summary\"" ws ":" ws string ws "," ws "\"tweet\"" ws ":" ws string ws "}" ws
string     ::= "\"" chars "\"" ws
chars      ::= | char chars
char       ::= normal | escape
normal     ::= [^"\\\u0000-\u001F]
escape     ::= "\\" ( ["\\/bfnrt] | "u" hex hex hex hex )
hex        ::= [0-9a-fA-F]
ws         ::= | ws (" " | "\t" | "\r" | "\n")
